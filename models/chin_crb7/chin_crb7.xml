    	<general name="joint2" joint="joint2" ctrlrange="-100 100"/>
    	<general name="joint3" joint="joint3" ctrlrange="-100 100"/>
    	<general name="joint4" joint="joint4" ctrlrange="-50 50"/>
    	<general name="joint5" joint="joint5" ctrlrange="-50 50"/>
